<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="head.additional">
            <block class="Ebizmarts\MailChimp\Block\Mailchimpjs" template="Ebizmarts_MailChimp::mailchimpjs.phtml" ifconfig="mailchimp/general/active" />
        </referenceBlock>
        <referenceContainer name="content">
            <block class="Ebizmarts\MailChimp\Block\Catcher" template="Ebizmarts_MailChimp::catcher.phtml" ifconfig="mailchimp/general/active"/>
        </referenceContainer>
    </body>
</page>
